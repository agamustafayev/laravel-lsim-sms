{
    "name": "samirmh-dev/laravel-lsim-sms",
    "description": "LSIM SMS provayderinden istifade ederek Laravel ile SMS gondermek ucun paket",
    "keywords": [
        "lsim",
        "laravel",
        "sms gonder",
        "send sms",
        "laravel sms",
        "php",
        "sendsms.az"
    ],
    "homepage": "https://github.com/samirmh-dev/laravel-lsim-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Samir Mammadhasanov",
            "email": "hello@samirmh.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.5",
        "illuminate/config": "^5.5 || ^6 || ^7",
        "illuminate/database": "^5.5 || ^6 || ^7",
        "illuminate/support": "^5.5 || ^6 || ^7",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "samirmhdev\\LaravelLsimSms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "samirmhdev\\LaravelLsimSms\\LsimServiceProvider"
            ],
            "aliases": {
                "LaravelLsimSms": "samirmhdev\\LaravelLsimSms\\Facades\\LsimFacade"
            }
        }
    }
}
